/Users/marcin/Desktop/flask-react-blog/frontend/src/views/NotLogged.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/NavBar/NavBarNotLogged.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/Auth/Login/Login.js",["88"],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/Auth/SignUp/SignUp.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/services/gradient.js",["89"],"const hash = require('string-hash')\nconst color = require('tinycolor2')\n\nexport const gradient = (uid, type = 'diagonal', opacity = 1) => {\n    if (uid) {\n        const n = hash(uid)\n        let c1 = color({h: n % 360, s: 0.95, l: 0.5, a: opacity})\n        let c1_ = c1.toRgbString()\n        let c2 = c1.triad()[1]\n        c2.setAlpha(opacity)\n        c2 = c2.toRgbString()\n\n        switch (type) {\n            case 'diagonal':\n                return `linear-gradient(to top right, ${c1_}, ${c2})`\n\n            case 'radial':\n                return `radial-gradient(circle, ${c1_}, ${c2})`\n\n            case 'horizontal':\n                return `linear-gradient(${c1_}, ${c2})`\n\n            case 'vertical':\n                return `linear-gradient(to right, ${c1_}, ${c2})`\n\n            default:\n                return `linear-gradient(to top right, ${c1_}, ${c2})`\n        }\n    }\n\n    throw new TypeError('uid is required')\n}\n\nexport function randomString(length) {\n    let result = '';\n    const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\n    const charactersLength = characters.length;\n    for (let i = 0; i < length; i++) {\n        result += characters.charAt(Math.floor(Math.random() * charactersLength));\n    }\n    return result;\n}\n\nexport function randomType() {\n    const words = [\"diagonal\", \"vertical\", \"horizontal\", \"radial\"];\n    return words[Math.floor(Math.random() * words.length)]\n}\n\n\n\n\nfunction randomGradient(opacity) {\n    const types = [\"linear-gradient\", \"radial-gradient\"];\n    let type = types[Math.floor(Math.random() * types.length)];\n    const options = [\"to top\", \"to left\", \"to bottom left\"]\n    let option = options[Math.floor(Math.random() * options.length)];\n    let val = [];\n    let min = Math.ceil(50);\n    let max = Math.floor(255);\n    for (let i = 0; i <= 6; i++) {\n        val.push(Math.floor(Math.random() * (max - min + 1)) + min);\n    }\n    if (type === 'radial-gradient') option = \"circle at center\"\n\n    // return type + '(' + option + ', rgba(' + val[0] + ', ' + val[1] + ', ' + val[3] + ', ' + opacity + '), ' +\n    // 'rgba(' + val[4] + ', ' + val[5] + ', ' + val[6] + ', ' + opacity + '))'\n\n    return type + '(' + option + ',' + getRandomColor() + ',' + getRandomColor() + ')'\n}\n\nfunction getRandomColor() {\n    const trans = '1';\n    let color = 'rgba(';\n    for (let i = 0; i < 3; i++) {\n        let min = Math.ceil(50);\n        let max = Math.floor(255);\n        color += Math.floor(Math.random() * (max - min + 1)) + min + ',';\n    }\n    color += trans + ')';\n    return color;\n}","/Users/marcin/Desktop/flask-react-blog/frontend/src/components/Auth/SignUp/layout.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/Home/Post/Post.js",["90"],"import React from \"react\";\nimport {gradient, randomType} from \"../../../services/gradient\";\nimport moment from \"moment\";\n\nimport \"./layout.css\"\n\nexport const Post = props => {\n\n    const bgGradient = { background: gradient(props.title, \"diagonal\", 0.1)}\n    const date = moment(props.date)\n\n    return (\n        <div className=\"post\" style={bgGradient}>\n            <time className=\"date\">\n                <span className=\"date__day\">{date.date()}</span>\n                <span className=\"date__month_year\">{date.format('MMM')} {date.year()}</span>\n                <span className=\"date__hours\">{date.format('h:m:s')}</span>\n            </time>\n            <div className=\"post_content\">\n                <div className=\"post_title\">\n                    {props.title}\n                </div>\n                <div className=\"post_body\">\n                    {props.body}\n                </div>\n            </div>\n        </div>\n    )\n}","/Users/marcin/Desktop/flask-react-blog/frontend/src/views/Logged.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/NavBar/NavBarLogged.js",["91"],"/Users/marcin/Desktop/flask-react-blog/frontend/src/services/UserContext.js",[],"/Users/marcin/Desktop/flask-react-blog/frontend/src/components/AddPost/AddPost.js",["92"],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","severity":1,"message":"98","line":2,"column":9,"nodeType":"99","messageId":"100","endLine":2,"endColumn":14},{"ruleId":"97","severity":1,"message":"101","line":4,"column":29,"nodeType":"99","messageId":"100","endLine":4,"endColumn":40},{"ruleId":"97","severity":1,"message":"102","line":6,"column":16,"nodeType":"99","messageId":"100","endLine":6,"endColumn":23},{"ruleId":"97","severity":1,"message":"103","line":6,"column":25,"nodeType":"99","messageId":"100","endLine":6,"endColumn":33},{"ruleId":"104","severity":1,"message":"105","line":47,"column":8,"nodeType":"106","endLine":47,"endColumn":14,"suggestions":"107"},{"ruleId":"97","severity":1,"message":"108","line":17,"column":13,"nodeType":"99","messageId":"100","endLine":17,"endColumn":17},{"ruleId":"97","severity":1,"message":"109","line":52,"column":10,"nodeType":"99","messageId":"100","endLine":52,"endColumn":24},{"ruleId":"97","severity":1,"message":"110","line":2,"column":19,"nodeType":"99","messageId":"100","endLine":2,"endColumn":29},{"ruleId":"97","severity":1,"message":"111","line":15,"column":20,"nodeType":"99","messageId":"100","endLine":15,"endColumn":27},{"ruleId":"97","severity":1,"message":"111","line":7,"column":18,"nodeType":"99","messageId":"100","endLine":7,"endColumn":25},"no-native-reassign",["112"],"no-negated-i